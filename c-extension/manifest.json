{
    "manifest_version": 3,
    "name": "Chloe Extension",
    "version": "1.0.0",
    "description": "A basic template for a Chrome extension using TypeScript.",
    "permissions": [
        "downloads",
        "storage",
        "activeTab",
        "scripting",
        "identity",
        "webNavigation",
        "tabs"
    ],
    "key": "",
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://*.linkedin.com https://*.run.app https://chloe-extension-102305464279.europe-west1.run.app http://localhost:* https://localhost:* ws://localhost:*;"
    },
    "content_scripts": [
        {
            "matches": ["https://*.linkedin.com/*"],
            "js": ["content.js"],
            "run_at": "document_end",
            "css": ["public/styles/styles.css"],
            "all_frames": false
        }
    ],
    "host_permissions": [
        "https://www.linkedin.com/*",
        "https://api.linkedin.com/*",
        "http://localhost:8000/*",
        "http://127.0.0.1:8000/*",
        "https://chloe-extension-102305464279.europe-west1.run.app/*"
    ],
    "web_accessible_resources": [
        {
            "resources": ["public/assets/fonts/*.woff2","public/assets/images/*"],
            "matches": ["https://*.linkedin.com/*"]
        }
    ],
    "action": {
        "default_popup": "public/popup.html",
        "default_icon": {
            "16": "public/favicon-16x16.png",
            "32": "public/favicon-32x32.png"
        }
    },
    "options_page": "public/options.html"
}
